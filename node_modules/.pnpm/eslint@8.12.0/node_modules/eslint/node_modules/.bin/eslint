#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules/.pnpm/eslint@8.12.0/node_modules/eslint/bin/node_modules:/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules/.pnpm/eslint@8.12.0/node_modules/eslint/node_modules:/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules/.pnpm/eslint@8.12.0/node_modules:/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules/.pnpm/node_modules:/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules:/Users/wobsoriano/Documents/tests/node_modules:/Users/wobsoriano/Documents/node_modules:/Users/wobsoriano/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules/.pnpm/eslint@8.12.0/node_modules/eslint/bin/node_modules:/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules/.pnpm/eslint@8.12.0/node_modules/eslint/node_modules:/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules/.pnpm/eslint@8.12.0/node_modules:/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules/.pnpm/node_modules:/Users/wobsoriano/Documents/tests/vue-cli-plugin-pinia/node_modules:/Users/wobsoriano/Documents/tests/node_modules:/Users/wobsoriano/Documents/node_modules:/Users/wobsoriano/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/eslint.js" "$@"
else
  exec node  "$basedir/../../bin/eslint.js" "$@"
fi
